{
    "name": "konekt/resource",
    "description": "API Resources library to transform entities/models into REST API objects. Inspired by Laravel - usable in any Framework",
    "type": "library",
    "license": "MIT",
    "keywords": ["rest", "api", "resource", "konekt", "artkonekt"],
    "support": {
        "source": "https://github.com/artkonekt/resource"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8 - 10"
    },
    "autoload": {
        "psr-4": { "Konekt\\Resource\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Resource\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
